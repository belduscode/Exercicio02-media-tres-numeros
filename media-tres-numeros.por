programa {
  funcao inicio() {
    real num1, num2, num3, media_numeros

    escreva("Digite o 1º número: ")
    leia(num1)

    escreva("Digite o 2º número: ")
    leia(num2)

    escreva("Digite o 3º número: ")
    leia(num3)

    media_numeros = (num1 + num2 + num3) / 3

    escreva("A média dos números é: ", media_numeros)
  }
}
